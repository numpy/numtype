{
  "[markdown]": {
    "editor.rulers": [88],
  },
  "[python]": {
    "editor.rulers": [88, 120],
  },
  "[toml]": {
    "editor.rulers": [88],
  },
  "evenBetterToml.schema.enabled": false,
  "evenBetterToml.taplo.configFile.path": ".taplo.toml",
  "files.readonlyInclude": {
    "**/generated/*.pyi": true,
  },
  "git.branchProtection": ["main"],
  "markdownlint.configFile": "docs/.markdownlint.yaml",
  "mypy-type-checker.path": ["uv", "run", "--no-editable", "--no-sync", "mypy"],
}
